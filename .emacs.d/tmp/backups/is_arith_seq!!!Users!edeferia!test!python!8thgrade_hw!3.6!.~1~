#!/usr/bin/env python

import sys

def is_arith_seq( seq ):
    ath = 0
    prev_d=-1
    curr_d=-1

    for i in seq[1:]:
        curr_d = i - seq[ath]

        if ath == 0:
            prev_d = curr_d

        if curr_d != prev_d:
            break
        else:
            prev_d = curr_d
        ath = ath + 1

    if prev_d == curr_d:
        print 'Yay, detected arithmetic sequence!!!\n'
    else:
        print 'Oh uh, this is not an arithmetic sequence\n'

def read_seq():
    if len(sys.argv) != 2:
        print "You must provide a sequence, like '3,5,7,9' as input"
        exit(-1);
    else:
        seq_text = sys.argv[1].split(',')
        seq_int = [int(a) for a in seq_text]
    return seq_int


is_arith_seq(read_seq())
