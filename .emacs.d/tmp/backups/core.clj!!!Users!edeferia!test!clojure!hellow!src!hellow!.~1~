(ns hellow.core
  (:gen-class))

(use '[clojure.java.shell :only [sh]])


(defn test-me
  "This is a test-me function.
  The purpose of this function is to illustrate
  the simplicity with which functional programming
  can solve complex problems."
  []
  (let [some-col (sh "ls")] (clojure.string/split  (some-col :out) #"\n")  ) )


(defn -main
  "I don't do a whole lot ... yet."
  [& args]
  (println "Hello, World!") (test-me))
