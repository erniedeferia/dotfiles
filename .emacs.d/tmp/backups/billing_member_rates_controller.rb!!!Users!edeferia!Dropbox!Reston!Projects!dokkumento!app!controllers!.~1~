class BillingMemberRatesController < AlexandriaCloudController
  include TermsOfServicesAgreement
  respond_to :json, :only => [:index]

  defaults :resource_class => BillingMemberRate

 def index
   @billing_member_rates = collection
   respond_to do |format|
     format.json {render :json => @billing_entries, :layout => false}
     format.html {render 'index'}
   end
 end
end
